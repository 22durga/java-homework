import java.io.*;
import java.util.*;
public class Main
{
	public static void main(String[] args) 
	{
	    Scanner s=new Scanner(System.in);
	    int n=s.nextInt();
		int num=1,count=0;
		
		for(int i=0;i<=n;i++)
		{
		    num=i;
		    while(num>0)
		    {
		        int digit=num%10;
		        if(digit==1)
		        {
		            count++;
		        }
		        num=num/10;
		    }
		   
		}
		System.out.println("count isÂ "+count);
	}
}
